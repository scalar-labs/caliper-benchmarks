{
  "caliper" : {
    "blockchain": "scalardl",
    "command" : {
      "start": "/bin/sh networks/scalardl/scripts/up.sh 30",
      "end" : "/bin/sh networks/scalardl/scripts/down.sh"
    }
  },
  "contract": { "path": "src/scalardl" },
  "scalardl": {
    "network": {
      "client_properties": {
        "host": "localhost",
        "port": "50051",
        "privileged_port": "50052",
        "cert_holder_id": "test",
        "cert_version": 1,
        "cert_path": "networks/scalardl/certs/client.pem",
        "private_key_path": "networks/scalardl/certs/client-key.pem",
        "tls_enabled": false
      }
    },
    "contracts": [
      {
        "id": "CreateUser",
        "name": "com.example.contract.token.CreateUser",
        "fcn": "create_user",
        "path": "build/classes/java/main/com/example/contract/token/CreateUser.class"
      },
      {
        "id": "IssueToken",
        "name": "com.example.contract.token.IssueToken",
        "fcn": "issue_token",
        "path": "build/classes/java/main/com/example/contract/token/IssueToken.class"
      },
      {
        "id": "SendToken",
        "name": "com.example.contract.token.SendToken",
        "fcn": "send_token",
        "path": "build/classes/java/main/com/example/contract/token/SendToken.class"
      },
      {
        "id": "ExchangeToken",
        "name": "com.example.contract.token.ExchangeToken",
        "fcn": "exchange_token",
        "path": "build/classes/java/main/com/example/contract/token/ExchangeToken.class"
      }
    ]
  }
}
